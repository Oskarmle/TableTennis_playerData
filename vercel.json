{
    "version": 2,
    "builds": [
      {
        "src": "api/playerServer.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/players.js" },
      { "src": "/(.*)", "dest": "/public/index.html" }
    ]
  }
  